    "@tool(\"generate_qr\")\n",
    "def generate_qr_tool(data: str, filename: str) -> str:\n",
    "    \"\"\"Generates a QR code image for the booking and returns the file path.\"\"\"\n",
    "    img = qrcode.make(data)\n",
    "    path = f\"./{filename}.png\"\n",
    "    img.save(path)\n",
    "    return path"
   ]
